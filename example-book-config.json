{
  "app": "Wikistr",
  "type": "book-config",
  "name": "torah",
  "displayName": "Torah",
  "books": {
    "Genesis": ["Gen", "Ge", "Gn"],
    "Exodus": ["Exod", "Ex", "Exo"],
    "Leviticus": ["Lev", "Le", "Lv"],
    "Numbers": ["Num", "Nu", "Nm", "Nb"],
    "Deuteronomy": ["Deut", "De", "Dt"]
  },
  "versions": {
    "JPS": "Jewish Publication Society",
    "TANAKH": "Tanakh",
    "HEBREW": "Hebrew Text",
    "SEPTUAGINT": "Septuagint"
  },
  "parsingRules": {
    "bookPattern": "^(?:Genesis|Exodus|Leviticus|Numbers|Deuteronomy)\\s+\\d+(?::\\d+)?(?:\\s*[-,]\\s*\\d+)*$",
    "chapterPattern": "^\\d+$",
    "versePattern": "^\\d+(?:[-,\\s]\\d+)*$",
    "versionPattern": "^(JPS|TANAKH|HEBREW|SEPTUAGINT)$"
  },
  "displayFormat": {
    "bookChapterVerse": "default",
    "withVersion": "default"
  }
}
